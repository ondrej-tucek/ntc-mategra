{
	"name": "ntc-mategra",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build:gh": "set NODE_ENV=production && rm -rf build && svelte-kit build && touch build/.nojekyll && cp CNAME build/",
		"deploy": "npm run build && npx gh-pages -d build -t true",
		"dev": "set NODE_ENV=development && svelte-kit dev",
		"start": "svelte-kit start",
		"preview": "svelte-kit preview",
		"build": "set NODE_ENV=production && svelte-kit build",
		"rebuild": "rm -rf build && svelte-kit build",
		"reinstall": "rm -rf node_modules && npm i",
		"pkg:info": "ncu && npm outdated",
		"pkg:up": "ncu -u && npm i && npm up"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"gh-pages": "^3.2.0",
		"svelte": "^3.38.2"
	}
}
